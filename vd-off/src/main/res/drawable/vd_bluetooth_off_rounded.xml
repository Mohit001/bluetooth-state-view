<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:tint="?attr/colorControlNormal"
    android:viewportWidth="24.0"
    android:viewportHeight="24.0">

    <path
        android:name="strike_through"
        android:fillColor="#FF000000"
        android:pathData="@string/bluetooth_off_path_strike_through_rounded" />

    <group>
        <clip-path
            android:name="bluetooth_icon_mask"
            android:pathData="@string/bluetooth_off_clip_path_state_off" />
        <path
            android:name="bluetooth_icon"
            android:fillColor="#FF000000"
            android:pathData="@string/bluetooth_path_rounded" />
    </group>

</vector>
