<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:tint="?attr/colorControlNormal"
    android:viewportWidth="24.0"
    android:viewportHeight="24.0">

    <group android:translateX="@integer/bluetooth_searching_icon_translation_x">
        <path
            android:name="bluetooth_icon"
            android:fillColor="#FF000000"
            android:pathData="@string/bluetooth_icon_path_rounded" />
    </group>

    <path
        android:name="first_level"
        android:fillColor="#FF000000"
        android:pathData="@string/bluetooth_searching_path_first_level_rounded" />

    <path
        android:name="second_level"
        android:fillColor="#FF000000"
        android:pathData="@string/bluetooth_searching_path_second_level_rounded" />

</vector>
